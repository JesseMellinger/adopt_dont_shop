<%= link_to "Pet Index", "/pets", method: "get" %>

<form action="/shelters" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter new shelter information:</p>
  <label for="name">Name:</label>
  <input type="text" name="shelter[name]" id="name"/><br/>
  <label for="address">Address:</label>
  <input type="text" name="shelter[address]" id="address"/><br/>
  <label for="city">City:</label>
  <input type="text" name="shelter[city]" id="city"/><br/>
  <label for="state">State:</label>
  <input type="text" name="shelter[state]" id="state"/><br/>
  <label for="zip">Zip:</label>
  <input type="text" name="shelter[zip]" id="zip"/><br/>
  <button type="submit" id="create_shelter"/>Create Shelter</button>
</form>
